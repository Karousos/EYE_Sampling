<Window x:Class="EYE_Sampling.Window_Praxi"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Επιλογή Πράξεων" Height="300" Width="300" Topmost="True" WindowStartupLocation="CenterOwner" WindowStyle="ToolWindow" Loaded="Window_Praxi_Loaded" Closed="Window_Praxi_Closed">
    <Grid>
        <ListBox x:Name="LB_Praxeis" HorizontalAlignment="Left" Height="213" Margin="10,10,0,0" VerticalAlignment="Top" Width="272" SelectionMode="Multiple">
            <ListBoxItem Content="Πράξη 1"/>
            <ListBoxItem Content="Πράξη 2"/>
            <ListBoxItem Content="Πράξη 3"/>
        </ListBox>
        <Button Content="OK" Height="32"  x:Name="BT_OK" 
        	VerticalAlignment="Top" BorderThickness="0,0,0,0"
        	Background="Transparent" Margin="177,228,0,0" HorizontalAlignment="Left" Width="105" Click="BT_OK_Click"/>
        <Button Content="Άκυρο" Height="32"  x:Name="BT_Cancel" 
        	VerticalAlignment="Top" BorderThickness="0,0,0,0"
        	Background="Transparent" Margin="10,228,0,0" HorizontalAlignment="Left" Width="105" Click="BT_Cancel_Click"/>

    </Grid>
</Window>
